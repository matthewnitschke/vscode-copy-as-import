{
  "name": "vscode-copy-as-import",
  "displayName": "Copy as Import",
  "description": "Provides ability to copy files as their import string",
  "publisher": "matthew-nitschke",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copy-as-import.copy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewnitschke/vscode-copy-as-import"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-as-import.copy",
        "title": "Copy as Import"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "when": "resourceLangId == dart",
          "command": "copy-as-import.copy",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == dart",
          "command": "copy-as-import.copy",
          "group": "6_copypath"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "yaml": "^2.1.2"
  }
}
